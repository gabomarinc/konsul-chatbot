# Configuración de GPTMaker API
# Copia este archivo como .env.local y configura tus variables

# Token de GPTMaker API
GPTMAKER_API_TOKEN=tu_token_aqui

# URL base de GPTMaker API
GPTMAKER_BASE_URL=https://api.gptmaker.com

# Configuración de Airtable (DEPRECADO - Migrando a Neon)
AIRTABLE_API_KEY=tu_api_key_aqui
AIRTABLE_BASE_ID=tu_base_id_aqui

# Configuración de Neon Database
NEON_DATABASE_URL=postgresql://user:password@host/database?sslmode=require

# Configuración de Stripe
STRIPE_PUBLISHABLE_KEY=pk_test_...
STRIPE_SECRET_KEY=sk_test_...

# Configuración del servidor
PORT=3001
NODE_ENV=development

